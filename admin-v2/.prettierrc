{
   "arrowParens": "always",
   "bracketSameLine": false,
   "bracketSpacing": true,
   "embeddedLanguageFormatting": "auto",
   "htmlWhitespaceSensitivity": "css",
   "insertPragma": false,
   "printWidth": 80,
   "quoteProps": "as-needed",
   "requirePragma": false,
   "semi": false,
   "singleQuote": true,
   "tabWidth": 3,
   "useTabs": false,
   "vueIndentScriptAndStyle": false,
   "endOfLine": "lf",
   "tsxSingleQuote": true,
   "trailingComma": "all"
}
